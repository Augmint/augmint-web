{
  "name": "augmint-web",
  "version": "0.1.0",
  "description": "Augmint Stable Tokens",
  "homepage": ".",
  "private": true,
  "engines": {
    "node": "10.15.3",
    "yarn": "1.15.2"
  },
  "dependencies": {
    "@augmint/js": "0.3.3",
    "bignumber.js": "5.0.0",
    "bn.js": "4.11.8",
    "chart.js": "2.8.0",
    "ethereumjs-util": "6.1.0",
    "ethers": "4.0.27",
    "moment": "2.24.0",
    "parse": "2.4.0",
    "react": "16.8.6",
    "react-copy-to-clipboard": "5.0.1",
    "react-dom": "16.8.6",
    "react-ga": "2.6.0",
    "react-redux": "7.1.0",
    "react-router-dom": "5.0.0",
    "react-scripts": "3.0.1",
    "react-scrollchor": "6.0.0",
    "react-tooltip": "3.10.0",
    "redux": "4.0.1",
    "redux-form": "8.2.3",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "redux-watch": "1.1.1",
    "stringifier": "2.0.0",
    "styled-components": "4.2.0",
    "styled-components-grid": "2.2.2",
    "styled-tools": "1.7.1",
    "web3": "1.0.0-beta.36"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:runrecord": "$(yarn bin)/cypress run --record --key 42b3c18f-e1cf-4c27-85ed-c223e420bd4a",
    "ganache:start": "yarn augmint-cli ganache start --blockTime 1",
    "ganache:stop": "yarn augmint-cli ganache stop",
    "ganache:run": "yarn augmint-cli ganache run --blockTime 1"
  },
  "devDependencies": {
    "cypress": "3.3.1",
    "eslint-plugin-cypress": "2.2.1",
    "husky": "2.1.0",
    "prettier": "1.18.0",
    "pretty-quick": "1.11.1",
    "wait-on": "3.2.0"
  },
  "resolutions": {
    "sha3": "2.0.2"
  },
  "greenkeeper": {
    "ignore": [
      "bignumber.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
